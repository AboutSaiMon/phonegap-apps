<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='firstapp'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<!-- Inherit GWT Mobile UI module -->
	<inherits name='com.gwtmobile.ui.gwtmobile_ui' />
	<!-- Inherit GWT Mobile PhoneGap module -->
	<inherits name='com.gwtmobile.phonegap.gwtmobile_phonegap' />
	<!-- Specify the app entry point class. -->
	<entry-point class='spa.simone.firstapp.client.FirstApp' />
	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<set-property name="user.agent" value="safari" />
</module>
